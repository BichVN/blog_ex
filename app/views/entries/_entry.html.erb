<tr>
  <td class="entry">
  	<span class="title"><%= entry.title %></span>
    <span class="content"><%= entry.body %></span>
    <span class="timestamp">
      Posted <%= time_ago_in_words(entry.created_at) %> ago.    
    </span>
   </td>
   <%= render entry.comments %>
   <%= render 'shared/comment_form', entry: entry %>
</tr>
